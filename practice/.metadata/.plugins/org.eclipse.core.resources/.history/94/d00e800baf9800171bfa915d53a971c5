package practice1;

public class RotateMatrix {
	public int[][] rotate(int[][] matrix){
		int n = matrix.length;
		int m = matrix[0].length;
		if(matrix == null || n == 0  || m == 0){
			return matrix;
		}
		for(int i = 0 ; i < n ; i++){
			for(int j = 0 ; j < i ; j++){
				int temp = matrix[i][j];
				matrix[i][j] = matrix[j][i];
				matrix[j][i] = temp;
			}
		}
		for(int i = 0 ; i < n ; i++){
			for(int j = 0 ; j < m / 2 ; j++){
				int temp = matrix[i][j];
				matrix[i][j] = matrix[i][m - 1 - j];
				matrix[i][m - 1 - j] = temp;
			}
		}
		return matrix;
	}
}
