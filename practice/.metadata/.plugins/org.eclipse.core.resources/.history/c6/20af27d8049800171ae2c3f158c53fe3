package DP;

public class JumpGame2 {
	public int Jump(int[] array){
		int n = array.length;
		int[] result = new int[n + 1];
		if(array == null){
			return -1;
		}
		if(array.length == 0 || array.length == 1){
			return 0;
		}
		result[0] = 0;
		result[1] = 0;
		
		for(int i = 2 ; i <= array.length ; i++ ){
			result[i] = -1;
			for(int j = i - 1 ; j >= 0 ; j++){
				if(array[j] + j >= i && result[j] != -1){
				if(result[i] == -1 || result[j] + 1 <= result[i]){
					result[i] = result[j] + 1;
				}
			}
		}
		return result[n];
	}
}
