package IXL;

public class solution {
	public void printRightPoint(){
		for(int i = 1 ; i <= 100 ; i++){
			if(i % 3 == 0 && i % 5 == 0){
				System.out.println("rightpoint");
			}else if(i % 3 == 0){
				System.out.println("right");
			}else if(i % 5 == 0){
				System.out.println("point");
			}
		}
		return
	}
}
